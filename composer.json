{
    "name": "coredevsolutions/laravel-smartconnect",
    "description": "A Laravel wrapper for sending SMS through Smart Connect SMS API.",
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "nesbot/carbon": "~1.0",
        "illuminate/contracts": "5.5.*|5.6.*|5.7.*|5.8.*",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*",
        "php": "^7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "coreDev Solutions Inc.",
            "email": "info@coredev.ph"
        }
    ],
    "autoload": {
        "psr-4": {
            "CoreDev\\LaravelSmartConnect\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CoreDev\\LaravelSmartConnect\\SmartServiceProvider"
            ],
            "aliases": {
                "Globe": "CoreDev\\LaravelSmartConnect\\Smart"
            }
        }
    }
}
